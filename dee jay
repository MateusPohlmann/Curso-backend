let deeJay = new PersonagemImpl("João", 100, "Brasil", "Karate");
console.log(deeJay.getNome());  // João
console.log(deeJay.golpeLeve());  // Dano gerado aleatoriamente entre 1 e 15
console.log(deeJay.golpePesado());  // Dano gerado aleatoriamente entre 1 e 30
console.log(deeJay.golpeEspecial());  // Sempre entre 20 e 30