let honda = new PersonagemImpl("João", 100, "Brasil", "Karate");
console.log(honda.getNome());  // João
console.log(honda.golpeLeve());  // Dano gerado aleatoriamente entre 1 e 15
console.log(honda.golpePesado());  // Dano gerado aleatoriamente entre 1 e 30
console.log(honda.golpeEspecial());  // Sempre entre 20 e 30